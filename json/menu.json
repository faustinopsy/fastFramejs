{
"header": {
    "tipo": "header",
    "style": {
        "box-sizing": "border-box", 
        "background": "lightblue",
        "textAlign": "center",
        "height": "auto"
    }
},
"nav": {
    "tipo": "nav",
    "autocapitalize": "a",
    "id": "nav",
    "style": {
        "box-sizing": "border-box", 
        "display": "flex", 
        "flex-wrap": "wrap",
        "alignItems": "center", 
        "height": "50px"
    }
},
"ul": {
    "tipo": "ul",
    "style": {
        "box-sizing": "border-box", 
        "list-style": "none",
        "margin": "0", 
        "flex": "1"
    }
},
"li": {
    "tipo": "li",
    "style": {
        "box-sizing": "border-box", 
        "flex": "1"
    }
},
"logo": {
    "tipo": "img",
    "src": "./img/icone.png",
    "style": {
        "box-sizing": "border-box", 
        "cursor": "pointer",
        "text-decoration":"none",
        "position": "relative",
        "right": "40px",
        "width": "40px"
    },
    "textContent": "Home",
    "hover":{
        "colorin": "#33DCFF",
        "colorout": ""
      },
    "eventos": {
        "click": "navHome"
    }
},
"a1": {
    "tipo": "a",
    "hash" : "home",
    "accessKey": "h",
    "style": {
        "box-sizing": "border-box", 
        "cursor": "pointer",
        "text-decoration":"none",
        "padding": "15px"
    },
    "textContent": "Home",
    "hover":{
        "colorin": "#33DCFF",
        "colorout": ""
      },
